<mapper namespace="com.spring.mapper.DocumentMapper">

    <!-- 사용자 ID로 사용자 조회 -->
    <select id="selectDocumentInTitle" resultType="Document">
        SELECT *
        FROM "Doc"
        WHERE title LIKE '%' || #{word} || '%'
    </select>



</mapper>